								.col-auto
									+e.product-link.active(href='#'): +icon('heart')
								.col-auto
									+e.product-link(href='#'): +icon('bar')
							+e.product-types
								mixin productType(img)
									+in-c('radio', '', 'type')
										.img-wrap-center
											+img(img)
								+productType('img/sProductCard-img-sm-1.png')
								+productType('img/product-type-img-1.png')
